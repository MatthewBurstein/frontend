<nav class="hmcts-primary-navigation" aria-label="{{params.label}}">
  <div class="hmcts-primary-navigation__container hmcts-width-container">
    <ul class="hmcts-primary-navigation__list">
      {% for item in params.items %}
        <li class="hmcts-primary-navigation__item">
          <a class="hmcts-primary-navigation__link" {{ 'aria-current=page' if item.selected }} href="{{item.href}}">{{item.text}}</a>
        </li>
      {% endfor %}
    </ul>
  </div>
</nav>